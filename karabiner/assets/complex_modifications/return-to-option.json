{
  "title": "Enter to Option with other keys",
  "rules": [
    {
      "description": "Enter to Option with other keys",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_option"
            }
          ],
          "to_if_alone": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "conditions": [
            {
              "bundle_identifiers": [
                "^com\\.amazon\\.workspaces$"
              ],
              "type": "frontmost_application_unless"
            }
          ]
        }
      ]
    }
  ]
}
